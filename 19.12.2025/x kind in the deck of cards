class Solution {
    public boolean hasGroupsSizeX(int[] deck) {
        if(deck.length<=1) return false;
        Map<Integer,Integer> map=new HashMap<>();
        for(int i:deck)
        {
            map.put(i,map.getOrDefault(i,0)+1);
        }
        int gcd=0;
        for(int i:map.values())
        {
            gcd=findg(gcd,i);
        }
        return gcd>=2;
    }
    public static int findg(int a,int b)
    {
        if(b==0) return a;
        return findg(b,a%b);
    }
}
